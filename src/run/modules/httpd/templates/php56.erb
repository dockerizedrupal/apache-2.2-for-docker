<IfModule mod_fastcgi.c>
  AddHandler php .php

  Alias /php56 /httpd-2.2.22/php56
  FastCgiExternalServer /httpd-2.2.22/php56 -host <%= @php56_port_9000_tcp %> -idle-timeout 300 -pass-header Authorization

  <Location /php56>
    Order deny,allow
    Deny from all
    Allow from env=REDIRECT_STATUS
  </Location>

  Action php /php56
</IfModule>
