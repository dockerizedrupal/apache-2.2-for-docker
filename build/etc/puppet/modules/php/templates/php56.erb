<IfModule mod_fastcgi.c>
  AddHandler php .php

  Alias /php52 /apache-2.2.22/data/php56
  FastCgiExternalServer /apache-2.2.22/data/php56 -host <%= @php56_port %> -idle-timeout 300 -pass-header Authorization

  <Location /php56>
    Order deny,allow
    Deny from all
    Allow from env=REDIRECT_STATUS
  </Location>

  Action php /php56
</IfModule>
